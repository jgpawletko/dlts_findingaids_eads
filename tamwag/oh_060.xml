    <archdesc level="collection">
        <did>
            <unittitle>Colin Davis Waterfront Workers Oral History Collection</unittitle>
            <unitid>OH.060</unitid>
            <repository>
                <corpname>Tamiment Library and Robert F. Wagner Labor Archive</corpname>
            </repository>
            <langmaterial>
